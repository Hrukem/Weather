<!DOCTYPE html>
<html>
  <body>
    <h4> История погоды в Электростали </h4>
    <%= form_for @conn, Routes.history_path(@conn, :show), fn f -> %>
    <%= date_select(f, :historydate, year: [options: 2020..2021]) %>
    <p>
      <button type="submit" disabled style="display: none" ></button>
      <%= submit("Show weather") %>
    </p>

    <% end %>

    <%= button("Back", to: "/", method: :get) %>

  </body>
</html>
